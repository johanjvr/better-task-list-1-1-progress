<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/favicon.ico")">

    <link href="@Url.Content("~/Content/themes/custom-theme/jquery-ui-1.8.13.custom.css")" rel="stylesheet" type="text/css" />  
    <link href="@Url.Content("~/Content/Style.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.13.custom.min.js")" type="text/javascript"></script>

</head>

<!-- Mixpanel -->
<script type="text/javascript">
    var mpq = []; mpq.push(["init", "2d57205503df6520e577b14be0506654"]); (function () { var a = document.createElement("script"); a.type = "text/javascript"; a.async = true; a.src = (document.location.protocol === "https:" ? "https:" : "http:") + "//api.mixpanel.com/site_media/js/api/mixpanel.js"; var b = document.getElementsByTagName("script")[0]; b.parentNode.insertBefore(a, b) })();
</script>
<!-- End Mixpanel -->

<body class="page page-child page-template content-sidebar">
    <div id="wrap">

             <div id="nav">
                    <div class="wrap">
                        <ul class="nav superfish">
                            @if (User.Identity.IsAuthenticated)
                            {
                                  @Html.Partial("_HomeMenu")
        
                                <li class="page_item">@Html.ActionLink("Coworkers", "CoWorkers", "Home", new { area = "" }, null)</li>
                            }    
                            <li class="home">@Html.Partial("_LogOnPartial")</li>
                        </ul>
                    </div>
                </div>
        
        <div id="header">
            <div class="wrap">
                <div id="title-area">
                    <h1 id="title">@Html.ActionLink("Progress", "Queue", "Ticket", new { area = "Projects" }, null)</h1>
                </div>
                  @* @Html.Partial("_SearchBar")*@
            </div>
        </div>

       <div id="subnav">
            <div class="wrap">
                    <ul class="nav superfish sf-js-enabled" id="menu-secondary-navigation">
                        <li class="menu-item menu-item-type-taxonomy menu-item-object-category menu-item-23" id="menu-item-23">@Html.ActionLink("New Ticket", "Create", "Ticket", new { area = "Projects" }, null)</li>
            </ul>
            </div>
            <!-- end .wrap -->
       </div>
           
        <div class="wrap" style="width: 962px; margin: 0 auto;">
                @if (TempData["message"] != null) { @Html.Partial("_Message") }
        
                @if (TempData["errorMessage"] != null) { @Html.Partial("_ErrorMessage") }                                                      
        </div>
                    
        <div id="inner">
            <div id="content-sidebar-wrap">    
            @RenderBody()
           </div>
        </div>

        <div class="footer-widgets" id="footer-widgets">
            <div class="wrap">
         
        </div>
        <!-- end .wrap -->
        </div>

        <div id="footer" class="footer">
            <div class="wrap">
                <div class="gototop">
                    <p><a rel="nofollow" href="#wrap">Return to top of page</a></p>
                </div>
                
                <div class="creds">
                    <p>
                        Copyright &copy; 2011
                        @Html.ActionLink("Sign in", "LogOn", "Account", new { area = "" }, null)
                    </p>
                </div>

            </div>
        </div>


    </div>
</body>
</html>

<script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-21026358-2']);
    _gaq.push(['_trackPageview']);

    (function () {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

</script>


